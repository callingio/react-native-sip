{
    "name": "@callingio/react-native-sip",
    "version": "1.1.1",
    "description": "React native wrapper for jssip",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "rimraf dist && tsc --project .",
        "build:watch": "rimraf dist && tsc --project . --watch",
        "format": "prettier --write --ignore-path .gitignore \"{.,src/**}/{*.js,*.ts,*.md,*.json}\"",
        "lint": "tsc --project . --noEmit && tslint --project . && prettier-check --ignore-path .gitignore \"{.,src/**}/{*.js,*.ts,*.md,*.json}\"",
        "prepublish": "npm run build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/callingio/react-native-sip"
    },
    "keywords": [
        "react",
        "react-native",
        "sip",
        "jssip",
        "webrtc"
    ],
    "author": "nicholasareed",
    "contributors": [
        {
            "name": "Saji Xavier",
            "url": "https://github.com/sajipitz"
        }
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/callingio/react-native-sip"
    },
    "homepage": "https://github.com/callingio/react-native-sip",
    "dependencies": {
        "react-native": "^0.63.3",
        "react-native-incall-manager": "^3.3.0",
        "react-native-jssip": "^3.7.6",
        "react-native-webrtc": "^1.89.3"
    },
    "peerDependencies": {},
    "devDependencies": {
        "@types/prop-types": "^15.7.3",
        "@types/react": "^16.14.8",
        "@types/webrtc": "0.0.26",
        "prettier": "^2.1.1",
        "prettier-check": "^2.0.0",
        "react": "^16.13.1",
        "rimraf": "^3.0.2",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.15.0",
        "typescript": "^3.9.9"
    }
}
